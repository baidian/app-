/**app.wxss**/
page{
  height:100%;
  width:100%;
}
view,text{
  font-family:PingFangSC,SourceHanSansHWSC;
}
image{
  border:none;
}
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}
button::after{
  border: none;
}
.container {
  height: 100%;
  width:100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex; 
  -webkit-box-orient: vertical;
  -webkit-flex-direction: column;
  flex-direction: column;
  /* align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box; */
} 
.elp{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}